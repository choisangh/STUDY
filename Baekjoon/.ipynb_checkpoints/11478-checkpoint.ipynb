    "S=input()\n",
    "def check_str(x,n,len):\n",
    "    global ans_set\n",
    "    for i in range(len):\n",
    "        ans_set.add(x[i:i+n])\n",
    "len_s=len(S)\n",
    "ans_set=set()\n",
    "for i in range(1,len_s+1):\n",
    "    check_n=(len_s+1-i)\n",
    "    check_str(S,i,check_n)\n",
    "\n",
    "print(len(ans_set))"
   ]
